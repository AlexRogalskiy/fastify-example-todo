    "fastify-cors": "^0.2.0",
    "fastify-helmet": "^3.0.0",
    "fastify-mongodb": "^1.0.0",
    "fastify-plugin": "^1.4.0"
  },
  "devDependencies": {
    "standard": "^12.0.1",
    "tap": "^12.0.0"
  }
